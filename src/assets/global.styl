html, body, #app, .online-clip
  height 100%
body
  margin 0
  background #000000
  overflow-x hidden
.el-loading-spinner
  .path
    stroke #26CB51
  .el-loading-text
    color #26CB51
.message
  font-size 12px
  vertical-align bottom
  color #888888
  cursor default
::-webkit-scrollbar
  width 4px
  height 4px
::-webkit-scrollbar-thumb
  border-radius 5px
  background-color #4d4c4c
